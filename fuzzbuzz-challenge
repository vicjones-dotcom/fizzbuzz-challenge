const numbersArr = [1, 2, 3, 4, 5, 6];
for (let i = 0; i < 501; i++) {
  console.log(i);
  if (i % 2 !== 0) {
    console.log("FiZZBUZZ++");
  } else if (i % 3 == 0 && i % 5 == 0) {
    console.log("FIZZBUZZ");
  } else if (i % 3 == 0) {
    console.log("FIZZ");
  } else if (i % 5 == 0) {
    console.log("BUZZ");
  }
}
